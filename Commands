Switch> enable
Switch# configure terminal
Switch(config)# hostname ASw1  
ASw1(config)# ! A comment is any line beginning with an exclamation mark.
ASw1(config)# ! ASw ==> Access Switch | DSw ==> Distribution Switch | CSw ==> Core Switch

Switch(config)# enable secret love

ASw1(config)# line console 0
ASw1(config-line)# password faith
ASw1(config-line)# login
ASw1(config-line)# exit

ASw1(config)# line vty 0 15
ASw1(config-line)# password hope
ASw1(config-line)# login
ASw1(config-line)# transport input all 
ASw1(config-line)# exit

ASw1(config)# interface vlan 1
ASw1(config-if)# ip address 192.168.1.2 255.255.255.0
ASw1(config-if)# no shutdown
ASw1(config-if)# exit
ASw1(config)# ip default-gateway 192.168.1.1

ASw1# show ip interface brief
ASw1# show interfaces status

-------------------------------------


ASw1(config)# username wendell secret odom
ASw1(config)# username chris secret youdda

ASw1(config)# line console 0
ASw1(config-line)# login local
ASw1(config-line)# no password 

ASw1(config)# line vty 0 15
ASw1(config-line)# login local 
ASw1(config-line)# no password 
ASw1(config-line)# transport input all

ASw1(config)# ip domain name example.com
ASw1(config)# crypto key generate rsa
ASw1(config)# ip ssh version 2

